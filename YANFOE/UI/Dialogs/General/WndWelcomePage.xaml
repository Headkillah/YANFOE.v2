        <Image Height="100" HorizontalAlignment="Left" Margin="12,12,0,0" Name="image1" Stretch="Uniform" VerticalAlignment="Top" Width="230" Source="/YANFOE;component/Resources/Untitled-3.png" />
        <CheckBox Content="Never show this again." Height="16" HorizontalAlignment="Left" Margin="12,254,0,0" Name="chkNeverShowAgain" VerticalAlignment="Top" />
