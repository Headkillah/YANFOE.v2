        <Button Height="44" HorizontalAlignment="Left" Margin="373,226,0,0" Name="btnOK" VerticalAlignment="Top" Width="75" Click="btnOK_Click">
            <Image Source="/YANFOE;component/Resources/accept1.png" />
        </Button>
        <GroupBox Header="Welcome To YANFOE v2" Height="100" HorizontalAlignment="Left" Margin="12,120,0,0" Name="groupBox1" VerticalAlignment="Top" Width="236">
